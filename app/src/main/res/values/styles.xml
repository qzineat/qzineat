    </style>
    <style name="text_edit_text">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/text_edit_text_size</item>
        <item name="android:textColorHint">@color/edit_text_hint_color</item>
        <item name="fontPath">fonts/NotoSans-Regular.ttf</item>
    </style>

    <style name="foodRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/food_rating_bar_full</item>
        <item name="android:minHeight">23dp</item>
        <item name="android:maxHeight">25dp</item>
    </style>

    <style name="search_edit_text">
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorHint">@color/hint_color_light</item>
        <item name="fontPath">fonts/NotoSans-Regular.ttf</item>
        <item name="android:textSize">@dimen/text_title_text_size</item>
    </style>

    <style name="nav_drawer_background_wh">
        <item name="android:background">@android:color/white</item>
    </style>
    <style name="nav_drawer_swap_wh">
        <item name="android:background">@color/drawer_gray</item>
    </style>


    <style name="nav_drawer_background_gr">
        <item name="android:background">@color/drawer_gray</item>
    </style>
    <style name="nav_drawer_swap_gr">
        <item name="android:background">@android:color/white</item>
        <item name="fontPath">fonts/Roboto-Medium.ttf</item>
    </style>

    <style name="DrawerTextGray">
        <item name="android:textColor">@color/drawer_gray</item>
    </style>
    <style name="DrawerTextWhite">
        <item name="android:textColor">@android:color/white</item>
        <item name="fontPath">fonts/Roboto-Medium.ttf</item>
    </style>

    <style name="text_regular_34">
        <item name="fontPath">fonts/NotoSans-Regular.ttf</item>
        <item name="android:textSize">34sp</item>
    </style>

    <style name="text_regular_24">
        <item name="fontPath">fonts/Roboto-Light.ttf</item>
        <item name="android:textSize">30sp</item>
    </style>
    <style name="text_regular_16">
        <item name="fontPath">fonts/Roboto-Regular.ttf</item>
        <item name="android:textSize">16sp</item>
    </style>




    <style name="Widget.CardContent" parent="android:Widget">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">24dp</item>
        <item name="android:orientation">vertical</item>
    </style>


    <style name="QZinEatUI" />
    <style name="QZinEatUI.Button" parent="@android:style/Widget.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@drawable/button_background_selector</item>
        <item name="android:textColor">@color/qzin_ui_dark_bg_display_text</item>
        <item name="android:textSize">18sp</item>
    </style>



    <!-- Login:Start - DO NOT CHANGE -->

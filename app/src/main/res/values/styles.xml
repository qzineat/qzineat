    <item name="android:textSize">@dimen/text_title_bold_text_size</item>
    <item name="fontPath">fonts/Roboto-Bold.ttf</item>
    </style>
    <style name="text_lg_title_bl">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/text_large_title_text_size</item>
        <item name="fontPath">fonts/RobotoCondensed-Regular.ttf</item>
    </style>
    <style name="text_title_bl">
